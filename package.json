{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:analyze": "vite build --mode analyze",
        "build:production": "NODE_ENV=production vite build --minify --sourcemap false",
        "build:clean": "rm -rf public/build && npm run build:production",
        "dev": "vite",
        "dev:https": "vite --https",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:watch": "vitest --watch",
        "test:php": "composer test",
        "test:all": "concurrently \"npm run test:coverage\" \"composer test:coverage\"",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "format": "prettier --write \"resources/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "format:check": "prettier --check \"resources/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "test:api": "newman run tests/api/askproai-api-collection.json -e tests/api/environments/local.json",
        "test:api:ci": "newman run tests/api/askproai-api-collection.json -e tests/api/environments/ci.json --reporters cli,json --reporter-json-export tests/api/results.json",
        "test:performance": "k6 run tests/Performance/k6/load-test.js",
        "test:performance:stress": "k6 run tests/Performance/k6/stress-test.js",
        "test:performance:spike": "k6 run tests/Performance/k6/spike-test.js",
        "test:performance:soak": "k6 run tests/Performance/k6/soak-test.js",
        "coverage:merge": "node coverage-merge.js",
        "coverage:report": "npm run test:all && npm run coverage:merge",
        "optimize:check": "npm run lint && npm run format:check && npm run build:analyze",
        "precommit": "npm run lint:fix && npm run format",
        "prepare": "npm run build:clean"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/vite": "^4.0.0",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.5",
        "@types/react": "^19.0.7",
        "@types/react-dom": "^19.0.3",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "@vitest/coverage-v8": "^2.1.8",
        "@vitest/ui": "^2.1.8",
        "alpinejs": "^3.14.9",
        "autoprefixer": "^10.4.2",
        "axios": "^1.7.4",
        "concurrently": "^9.0.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.1.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0",
        "happy-dom": "^16.6.1",
        "laravel-vite-plugin": "^1.2.0",
